	.transparent_btn {
		position: relative;
		background-color: transparent;
		box-shadow: none;
		display: flex;
		justify-content: flex-start;
		&:hover {
			&:before {
				display: none;
			}
		}
		&:focus {
			&:before {
				display: none !important;
			}
			.v-ripple__container {
				position: absolute;
				// display: none !important;
				width: 38px;
				height: 38px;
				border-radius: 50%;
				top: 6px;
				left: 0;
			}
		}
	}
