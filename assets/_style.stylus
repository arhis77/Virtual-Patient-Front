#app {
	position: static;
	width: 100%;
	left: 0px;
	top: 0px;
	flex: none;
	order: 0;
	align-self: center;
	flex-grow: 0;
	margin: 0px 10px;
	.main_page_01 {
        .inner {
            height: auto;
            position: relative;
        }
      
        .page_met__02.inner {
           height: auto;
           padding-bottom: 154px;
        }
        .title_01 {
            position absolute;
            left: 0;
            top: -13px;
            background-size: contain;
            background-repeat: no-repeat;
            width 404px;
            height 104px;
            display: block;
            text-align: center;
            z-index: 2;
            .text {
                z-index: 2;
                color: #FF0606;
                font-size: 28px;
                line-height: 37px;
            }
            &:after {
                display: block;
                content: '';
                width 404px;
                height 104px;
                position: absolute;
                z-index: 1;
                top: 0;
                left: 0;
                background: url('./assets/title_01__bg.svg') no-repeat;
                background-size: contain;
            }
            &:before {
                display: block;
                content: '';
                width: 394px;
                height: 99px;
                position: absolute;
                z-index: -1;
                top: 12px;
                left: 12px;
                background: url('./assets/title_01__bg__after.svg') no-repeat;
                background-size: cover;
            }
        }
     
    }
    .profile,
    .profile__btn {
        color: #1956D7;
        font-family: Arial Nova;
        font-style: normal;
        font-weight: normal;
        font-size: 26px;
        line-height: 36px;
        text-transform: capitalize;
		@media (max-width: 1903px) {
			line-height: 27px;
		}
    }
