.student__case {
	.toggle_btn {
		max-width: 100%;
		text-align: left;
		@media (min-width: 1904px) {
			span br {
				display: none;
			}
		}
		@media (max-width: 1263px) {
			width: 100%;
			br {
				display: none;
			}
		}
	}	
	.symptoms {
		background: #eff3fd;
		border: 1px solid #aab5da;
		box-sizing: border-box;
		border-radius: 5px;
		
		.theme--light.v-tabs-items {
			background-color: #eff3fd;
		}
		.theme--light.v-tabs > .v-tabs-bar .v-tab:not(.v-tab--active) {
			color: #1956D7;
		}
		.v-tabs {
			width: 100%;
			.v-slide-group__content.v-tabs-bar__content {
				background-color: #eff3fd;
			}
			.v-item-group.theme--light.v-slide-group.v-tabs-bar.primary--text {
				width: 100%;
				.v-slide-group__content.v-tabs-bar__content {
					display: flex;
					justify-content: space-around;
				}
			}
			.v-tab {
				color: red;
				font-size: 24px;
				font-weight: 300;
				line-height: 36px;
				color: #333333;
				text-transform: capitalize;
				transition: none !important;
				@media (max-width: 1903px) {
					font-size: 16px;
				}	
				* {
					transition: none !important;
				}
			}
			
		}
	}
	
	.v-tabs-items {
		.first {
			.desc p {
				font-size: 20px;
				line-height: 38px;
			}
			@media (max-width: 1903px) {
				.col-lg-6 img {
					max-width: 100%;
				}
			}
		}
		.second {
			.theme--light.v-card {
				background-color: transparent;
			}
			.theme--light.v-data-table {
				background-color: transparent;
			}
			tbody {
				background-color: white;
				td {
					border: 1px solid #AAB5DA !important;
					border-bottom: 1px solid #AAB5DA !important;
					box-sizing: border-box;
					border-radius: 3px;
					height: 100%;
					.v-input__slot {
						display: flex !important;
						justify-content: center;
						align-items: center;
						width: 100%;
					}
				}
			}
		}
	}
}
