body .v-btn.toggle_btn {
	background: #F2F4FB;
	border: 1px solid #AAB5DA;
	box-sizing: border-box;
	border-radius: 10px;
	.v-btn__content {
		font-size: 21px;
		color: #324979;
	}
	&.red--text {
		.v-btn__content {
			color: red;
		}
	}
}
.table-standart {
		width: 100%;
        border-spacing: 10px !important;
        table-layout: fixed;
        width: 100%;
	.v-form {
		.v-label {
			display: inline-block;
			margin-bottom: 9px;
		}
	}
	table {
		border-spacing: 10px !important;
	}
	thead {
		td {
			font-size: 18px !important;
			color: #324979;
			line-height: 1.2;
			.sorted {
				background-image: url('./assets/arrow_down__blue--01.svg');
				background-repeat: no-repeat;
				background-position: center right;
				padding-right: 20px;
				background-size: 13px;
				color: #324979;
				text-decoration: none;
			}
			.sorted_revers {
				background-image: url('./assets/arrow_down__blue--reverse--01.svg');
				background-repeat: no-repeat;
				background-position: center right;
				padding-right: 20px;
				background-size: 13px;
				color: #324979;
				text-decoration: none;
			}
            span {
                cursor pointer
            }
			img {
				margin-left -10px
			}
		}
	}
	tbody {
		td {
			background: #F2F4FB;
			font-size: 18px !important;
			line-height: 29px;
			color: #67687C;
			border-radius: 4px;
			border: 1px solid #AAB5DA;
			border-bottom: 1px solid #AAB5DA !important;
			@media (max-width: 1263px) {
				font-size: 16px !important;
				line-height: 1.2;
			}
		}
	}
	
	.table_btn {
		width: 100%;
		height: 100% !important;
		font-size: 18px !important;
		background: linear-gradient(180deg, #FFFFFF 0%, rgba(255, 255, 255, 0) 100%), #F2F4FB;
		text-transform: capitalize;
		.img_1 {
			display: inline;
		}
		.img_2 {
			display: none;
		}
		&:before {
			display: none;
		}
		&:active,
		&:focus {
			box-shadow: none !important;
			.img_2 {
				display: inline;
			}
			.img_1 {
				display: none;
			}
		}
		&.edit {
			color: #0B9D4E;
			&:active,
			&:focus {
				color: white;
				background-color: #07AD54 !important;
				background: #07AD54;
			}
		}
		&.view {
			color: #324979;
			&:active,
			&:focus {
				color: white;
				background-color: #324979 !important;
				background: #324979;
			}
		}
		&.delete {
			&:active,
			&:focus {
				background-color: #FF0606 !important;
				background: #FF0606;
			}
		}
	}
}

.popup__white_bg {
	&.v-overlay {
		// min-height: 107%;
		min-height: 100vh;
		height: auto;
		top: -15px;
	}	
	&.v-overlay--active {
		.v-overlay__scrim {
			background-color: white !important;
			opacity: 1 !important;
		}
	}
	.v-overlay__content {
		padding-top: 45px;
		min-height: 100vh;
	}
	.close {
		box-shadow: none;
		right: 0;
		position: absolute;
		top: -165px;
		top: 0;
		@media (min-width: 1904px) {
			right: -265px;
		}
		@media (max-width: 1903px) {
			top: -180px;
		}
		@media (max-width: 1263px) {
			top: -200px;
		}
	}
	.v-form {
		.v-input__control {
			height: 65px;
		}
		.v-input__slot {
			background:#F2F4FB !important;
			border-radius: 4px;
			border: 1px solid #AAB5DA;
		}
		.v-icon {
			color: rgba(0, 0, 0, 0.54);
		}
		.img_1 {
			display: inline;
		}
		.img_2 {
			display: none;
		}
		&:before {
			display: none;
		}
		&:active,
		&:focus {
			box-shadow: none !important;
			.img_2 {
				display: inline;
			}
			.img_1 {
				display: none;
			}
		}
	}
}

